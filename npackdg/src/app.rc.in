#include <windows.h>

1 24 DISCARDABLE "npackdg.manifest"
IDI_ICON1 ICON DISCARDABLE "app.ico"

VS_VERSION_INFO VERSIONINFO
FILEVERSION    	@NPACKD_VERSION_MAJOR@,@NPACKD_VERSION_MINOR@,@NPACKD_VERSION_PATCH@,@NPACKD_VERSION_TWEAK@
PRODUCTVERSION 	@NPACKD_VERSION_MAJOR@,@NPACKD_VERSION_MINOR@,@NPACKD_VERSION_PATCH@,@NPACKD_VERSION_TWEAK@
FILEFLAGSMASK VS_FFI_FILEFLAGSMASK
#ifdef _DEBUG
FILEFLAGS 0x1L
#else
FILEFLAGS 0x0L
#endif
FILEOS VOS__WINDOWS32
FILETYPE VFT_DLL
FILESUBTYPE VFT2_UNKNOWN
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904E4"
        BEGIN
            VALUE "CompanyName",      "Npackd\0"
            VALUE "FileDescription",  "Npackd\0"
            VALUE "FileVersion",      "@NPACKD_VERSION_MAJOR@.@NPACKD_VERSION_MINOR@.@NPACKD_VERSION_PATCH@.@NPACKD_VERSION_TWEAK@\0"
            VALUE "InternalName",     "Npackd\0"
            VALUE "LegalCopyright",   "Npackd\0"
            VALUE "LegalTrademarks1", "Npackd, GPLv3\0"
            VALUE "OriginalFilename", "@OUTPUT_FILE_NAME@\0"
            VALUE "ProductName",      "Npackd\0"
            VALUE "ProductVersion",   "@NPACKD_VERSION_MAJOR@.@NPACKD_VERSION_MINOR@.@NPACKD_VERSION_PATCH@.@NPACKD_VERSION_TWEAK@\0"
        END
    END

    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252
    END
END

